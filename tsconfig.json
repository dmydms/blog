{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "$assets": ["./src/assets"],
      "$assets/*": ["./src/assets/*"],
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],
      "$components": ["./src/components"],
      "$components/*": ["./src/components/*"]
    }
  }
}
